{
  "name": "ecomm",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration=local",
    "build": "ng build --configuration=dev",
    "build:dev": "ng build --configuration=dev && npm i -D typescript",
    "build:test": "ng build --configuration=test && npm i -D typescript",
    "build:stage": "ng build --configuration=stage && npm i -D typescript",
    "build:prod": "ng build --configuration=prod && npm i -D typescript",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --code-coverage",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless --code-coverage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.2.1",
    "@angular/cdk": "^13.2.1",
    "@angular/common": "^13.2.1",
    "@angular/compiler": "^13.2.1",
    "@angular/core": "^13.2.1",
    "@angular/flex-layout": "^13.0.0-beta.38",
    "@angular/forms": "^13.2.1",
    "@angular/material": "^13.2.1",
    "@angular/platform-browser": "^13.2.1",
    "@angular/platform-browser-dynamic": "^13.2.1",
    "@angular/router": "^13.2.1",
    "@ngrx/store": "^13.0.2",
    "@ngrx/store-devtools": "^13.0.2",
    "core-js": "^3.21.0",
    "file-saver": "^2.0.5",
    "rxjs": "~7.5.2",
    "tslib": "^2.3.1",
    "xlsx": "^0.18.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.2.1",
    "@angular/cli": "^13.2.2",
    "@angular/compiler-cli": "^13.2.1",
    "@types/file-saver": "^2.0.5",
    "@types/jasmine": "^3.10.3",
    "@types/lodash": "^4.14.178",
    "@types/node": "^12.12.62",
    "jasmine-core": "~4.0.0",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "typescript": "^4.5.5"
  }
}
